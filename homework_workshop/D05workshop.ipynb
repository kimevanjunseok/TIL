    "def Palindrome(word):\n",
    "    for i in range(len(word)//2):\n",
    "        if word[i] == word[-1*(i+1)]:\n",
    "            return 'Ture'\n",
    "        else:\n",
    "            return 'False'\n",
    "print(Palindrome('saippuakivikauppias'))         "
   ]
